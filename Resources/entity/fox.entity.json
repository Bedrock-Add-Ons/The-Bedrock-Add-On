{
	"format_version": "1.20.0",
	"minecraft:client_entity": {
		"description": {
			"identifier": "minecraft:fox",
			"geometry": { "default": "geometry.fox", "fennec": "geometry.fennec_fox" },
			"textures": {
				// Skins
				"red": "textures/entity/fox/fox",
				"arctic": "textures/entity/fox/arctic_fox",
				"silver": "textures/entity/fox/silver_fox",
				"cross": "textures/entity/fox/cross_fox",
				"fennec": "textures/entity/fox/fennec_fox",

				// Eyes
				"red_intelligent": "textures/entity/fox/fox_intelligent",
				"arctic_intelligent": "textures/entity/fox/arctic_fox_intelligent",
				"silver_intelligent": "textures/entity/fox/silver_fox_intelligent",
				"cross_intelligent": "textures/entity/fox/cross_fox_intelligent",
				"fennec_intelligent": "textures/entity/fox/fennec_fox_intelligent"
			},
			"materials": { "default": "fox" },
			"render_controllers": [
				"controller.render.fox"
			],
			//
			"scripts": {
				"pre_animation": [
					"variable.is_intelligent = query.property('yanasakana:intelligent');",
					"variable.habitat = query.property('yanasakana:habitat_variant');"
				],
				"animate": [
					"setup",
					{ "baby_transform": "query.is_baby" },
					"move"
				]
			},
			"animations": {
				"walk": "animation.quadruped.walk",
				"look_at_target": "animation.common.look_at_target",
				"baby_transform": "animation.fox.baby_transform",
				"crouch": "animation.fox.crouch",
				"sleep": "animation.fox.sleep",
				"setup": "animation.fox.setup",
				"sit": "animation.fox.sit",
				"wiggle": "animation.fox.wiggle",
				"stuck": "animation.fox.stuck",
				"pounce": "animation.fox.pounce",
				"move": "controller.animation.fox.move"
			},
			"spawn_egg": {
				"texture": "spawn_egg",
				"texture_index": 55
			}
		}
	}
}